class Codechef
{
	public static void main (String[] args)
	{
		Scanner sc = new Scanner(System.in);
		int t = sc.nextInt();
		while(t-->0)
		{
    		int a = sc.nextInt();
    		int b = sc.nextInt();
    		int c = a + b;
    		System.out.println(c);
		}
		
	}
}
/* Sample 1:
Input
3
1 2
100 200
10 40
Output
3
300
50 */
